{
  "version": 2,
  "framework": "vue",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)\\.(.+)",
      "headers": { "cache-control": "max-age=31536000, immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}